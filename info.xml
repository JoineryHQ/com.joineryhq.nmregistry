<?xml version="1.0"?>
<extension key="nmregistry" type="module">
  <file>nmregistry</file>
  <name>New Mexico Registry</name>
  <description>Custom features for the New Mexico respite providers registry</description>
  <license>GPL-3.0</license>
  <maintainer>
    <author>Allen Shaw</author>
    <email>allen@JoineryHQ.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/JoineryHQ/com.joineryhq.nmregistry</url>
    <url desc="Documentation">https://github.com/JoineryHQ/com.joineryhq.nmregistry</url>
    <url desc="Support">https://joineryhq.com</url>
    <url desc="Licensing">http://www.gnu.org/licenses/gpl-3.0.html</url>
  </urls>
  <releaseDate>2023-01-18</releaseDate>
  <version>0.1</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.0</ver>
  </compatibility>
  <comments>Developed by Joinery for NMDCC</comments>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Nmregistry</namespace>
    <format>22.10.0</format>
    <angularModule>crmNmregistry</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>ang-php@1.0.0</mixin>
  </mixins>
</extension>
